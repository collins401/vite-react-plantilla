{
  "name": "vite-react-plantilla",
  "private": true,
  "version": "0.1.2",
  "scripts": {
    "dev": "vite --host",
    "build:staging": "vite build --mode staging",
    "build": "vite build",
    "tsc": "tsc --noEmit",
    "preview": "vite preview",
    "eslint": "npx eslint . --ext .tsx,.jsx,.ts --fix src",
    "prettier": "prettier --write '**/**.{js,jsx,tsx,ts}'",
    "windicss": "npx windicss-analysis",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "release": "standard-version --preset gitmoji-config"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "lint-staged": {
    "*.{tsx,ts, json}": [
      "eslint --fix --ext",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "test.js"
    }
  },
  "engines": {
    "node": ">= 14.17.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "^6.33.0",
    "@reduxjs/toolkit": "^1.8.0",
    "ahooks": "^3.1.13",
    "antd": "^4.18.9",
    "axios": "^0.26.0",
    "history": "^5.3.0",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.21",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-react": "^1.2.0",
    "c8": "^7.11.0",
    "child_process": "^1.0.2",
    "commitlint": "^16.2.1",
    "commitlint-config-gitmoji": "^2.2.5",
    "consola": "^2.15.3",
    "conventional-changelog-cli": "^2.2.2",
    "conventional-changelog-gitmoji-config": "^1.4.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-plugin-react": "^7.29.2",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "less": "^4.1.2",
    "lint-staged": "^12.3.4",
    "path": "^0.12.7",
    "prettier": "^2.5.1",
    "rollup": "^2.68.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "standard-version": "^9.3.2",
    "typescript": "4.4.2",
    "vite": "^2.8.0",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vite-plugin-restart": "^0.1.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-windicss": "^1.8.1",
    "vitest": "^0.5.9"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@ant-design/icons",
        "@ant-design/pro-layout",
        "@reduxjs/toolkit",
        "antd",
        "antd/lib/locale/zh_CN",
        "js-cookie",
        "moment",
        "moment/locale/zh-cn",
        "nprogress",
        "react",
        "react-dom",
        "react-redux",
        "react-router-dom"
      ]
    }
  }
}
